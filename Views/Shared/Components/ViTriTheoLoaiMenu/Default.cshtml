@model IEnumerable<ViTri>
<div class="col-lg-12">
        <div class="section-title">
            <h2>Featured Product</h2>
        </div>
        <div class="featured__controls">
            <ul>
            <li class="active" data-filter="*">All</li>

            @foreach (var item in Model)
            {
                <li><a asp-area="Customer" asp-controller="Home" asp-action="SanPhamTheoViTri" asp-route-mavitri="@item.MaViTri">@item.TenViTri</a></li>
            }
            </ul>
        
        </div>
</div>
